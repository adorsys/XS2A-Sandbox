{
  "name": "oba-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "load-api": "bash load-api.sh",
    "swagger-gen": "npm run load-api && ng-swagger-gen -i ./src/app/api/swagger/oba.json -o ./src/app/api",
    "remove-and-swagger-gen": "rm -rf ./src/app/api && npm run swagger-gen",
    "start": "ng serve --port=4400 --proxy-config=proxy.conf.json",
    "start:proxy:dev": "ng serve --port=4400 --proxy-config=proxy.dev.conf.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "rm -rf ./src/app/api && npm run swagger-gen && ng test",
    "test-ci": "ng test --browsers ChromeHeadless --watch=false --environment=prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier --write",
    "prettier-check": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier-check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@auth0/angular-jwt": "^3.0.1",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.6.11",
    "css-vars-ponyfill": "^2.2.0",
    "date-fns": "^2.9.0",
    "http-status-codes": "^1.4.0",
    "material-icons": "^0.3.1",
    "ng-http-loader": "^6.0.1",
    "ngx-clipboard": "^12.3.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.4.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-swagger-gen": "^1.8.1",
    "popper.js": "^1.16.1",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "protractor": "^5.4.3",
    "puppeteer": "^1.13.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3"
  },
  "resolutions": {
    "mixin-deep": "latest"
  }
}

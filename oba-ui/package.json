{
  "name": "oba-ui",
  "version": "4.3.0",
  "scripts": {
    "ng": "ng",
    "load-api": "bash load-api.sh",
    "swagger-gen": "npm run load-api && ng-swagger-gen -i ./src/app/api/swagger/oba.json -o ./src/app/api",
    "remove-and-swagger-gen": "rm -rf ./src/app/api && npm run swagger-gen",
    "start": "ng serve --port=4400 --proxy-config=proxy.conf.json",
    "start:proxy:dev": "ng serve --port=4400 --proxy-config=proxy.dev.conf.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "rm -rf ./src/app/api && npm run swagger-gen && ng test",
    "test-ci": "ng test --browsers ChromeCi --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier --write",
    "prettier-check": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier-check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/forms": "^11.1.1",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "^11.1.1",
    "@angular/platform-browser-dynamic": "^11.1.1",
    "@angular/router": "^11.1.1",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "9.0.2",
    "bootstrap": "^4.6.0",
    "browser-detect": "^0.2.28",
    "core-js": "^3.8.3",
    "css-vars-ponyfill": "^2.4.3",
    "date-fns": "^2.17.0",
    "http-status-codes": "^2.1.4",
    "material-icons": "^0.3.1",
    "ng-http-loader": "9.1.0",
    "ngx-clipboard": "^12.3.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/language-service": "^11.1.1",
    "@angular/localize": "^11.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "husky": "^5.0.9",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.5.1",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-swagger-gen": "^2.3.0",
    "popper.js": "^1.16.1",
    "prettier": "^2.2.1",
    "prettier-check": "^2.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "^7.1.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.5"
  },
  "resolutions": {
    "mixin-deep": "latest"
  }
}
